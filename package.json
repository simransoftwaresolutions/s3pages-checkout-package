{
  "name": "s3pages-checkout",
  "version": "0.1.2",
  "description": "Checkout pages components of S3Pages",
  "main": "lib/exports.js",
  "types": "lib/exports.d.ts",
  "scripts": {
    "copy-preview": "copyfiles -u 1 src/styles/pagepreview/*.css lib/",
    "copy-checkout": "copyfiles -u 1 src/styles/* lib/",
    "copy-templategen": "copyfiles -u 1 src/styles/* lib/",
    "copy-slidersection": "copyfiles -u 1 src/styles/* lib/",
    "copy-bannersection": "copyfiles -u 1 src/styles/* lib/",
    "copy-pageTemplate": "copyfiles -u 1 src/styles/page-template/*.css lib/",
    "copy-files": "yarn copy-preview && yarn copy-checkout && yarn copy-templategen  && yarn copy-pageTemplate && yarn copy-slidersection && yarn copy-bannersection",
    "build": "tsc && yarn copy-files",
    "prepare": "npm run build",
    "start": "react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simransoftwaresolutions/s3pages-checkout-package.git"
  },
  "keywords": [
    "product checkout"
  ],
  "author": "Ram Ashish",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "typescript": "^4.9.5",
    "vite": "^4.4.11"
  },
  "files": [
    "lib/**/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "5.8.4",
    "@mui/joy": "^5.0.0-alpha.58",
    "@mui/material": "^5.10.2",
    "@types/bootstrap": "^5.2.3",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-bootstrap": "^0.32.30",
    "@types/react-dom": "^18.2.7",
    "animate.css": "^4.1.1",
    "axios": "^1.5.0",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "medium-editor-colorpicker-buttons": "^0.0.4",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-medium-editor": "^1.8.1",
    "react-quill": "^2.0.0",
    "react-scripts": "^5.0.1",
    "react-shimmer-effects": "^1.0.2",
    "react-toast-notifications": "^2.5.1",
    "react-toastify": "^9.1.3",
    "style-loader": "^3.3.3"
  }
}
